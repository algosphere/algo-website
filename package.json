{
  "name": "algosphere.org",
  "version": "1.0.0",
  "description": "Algosphere website",
  "main": "gulpfile.js",
  "scripts": {
    "start": "hugo server -DEF --i18n-warnings",
    "build-dev": "hugo --buildDrafts --buildExpired --buildFuture --environment preview --i18n-warnings && gulp dev",
    "build-prod": "hugo --i18n-warnings && gulp prod",
    "deploy": "netlify deploy --prod --dir public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algosphere/algo-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/algosphere/algo-website/issues"
  },
  "homepage": "https://github.com/algosphere/algo-website",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-imagemin": "^6.1.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-replace": "^0.6.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.1.0"
  },
  "dependencies": {
    "netlify-cli": "^2.49.0"
  }
}
