{
  "name": "algosphere.org",
  "version": "1.0.0",
  "description": "Algosphere website",
  "main": "gulpfile.js",
  "scripts": {
    "dev": "run-p _dev-hugo _dev-cms",
    "_dev-hugo": "hugo server -DEF --i18n-warnings",
    "_dev-cms": "netlify-cms-proxy-server",
    "_build": "hugo --i18n-warnings && gulp",
    "_build-dev": "hugo --buildDrafts --buildExpired --buildFuture --environment dev --i18n-warnings && gulp",
    "_deploy": "netlify deploy --prod --dir public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algosphere/algo-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/algosphere/algo-website/issues"
  },
  "homepage": "https://github.com/algosphere/algo-website",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "gulp": "^4.0.2",
    "gulp-pretty-html": "^2.0.10",
    "gulp-replace": "^1.1.3",
    "netlify-cms-app": "^2.15.59",
    "netlify-cms-proxy-server": "^1.3.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.3",
    "yargs-parser": ">=21.0.0"
  }
}
